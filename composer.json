{
    "name": "deesoft/yii2-angular",
    "description": "Yii2 angular extension",
    "keywords": ["yii2", "angularjs"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/deesoft/yii2-angular/issues",
        "source": "https://github.com/deesoft/yii2-angular"
    },
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "bower-asset/angular": "~1.3@stable",
        "bower-asset/angular-route": "~1.3@stable",
        "bower-asset/angular-resource": "~1.3@stable",
        "bower-asset/angular-bootstrap": "~0.13@stable",
        "bower-asset/angular-validation": "~1.3@stable"
    },
    "autoload": {
        "psr-4": {
            "dee\\angular\\": ""
        }
    },
    "extra": {
        "bootstrap": "dee\\angular\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "fxp-asset": {
            "pattern-skip-version": "(-build|-patch)"
        }
    }
}
